<Window x:Class="ADP2_Flight_Inspection_App.Graphs"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:oxy="http://oxyplot.org/wpf"
        xmlns:local="clr-namespace:ADP2_Flight_Inspection_App"
        mc:Ignorable="d"
        Title="Graphs" Height="450" Width="800">

    <Grid>
        <ListBox Name="listBox" HorizontalAlignment="Left" Height="401" VerticalAlignment="Top" Width="235" Margin="0,10,0,0" SelectionChanged="ListBox_selectionChanged">

        </ListBox>
        <oxy:PlotView Name="measure" HorizontalAlignment="Right" Height="201" VerticalAlignment="Top" Width="276" Margin="0,10,0.333,0" >
            <oxy:PlotView.Model >
                <Binding Path="MyModel" />
            </oxy:PlotView.Model>
        </oxy:PlotView>

        <oxy:PlotView Name="corMeasure" HorizontalAlignment="Right" Height="201" VerticalAlignment="Top" Width="276" Margin="0,10,277.333,0">
            <oxy:PlotView.Model >
                <Binding Path="corModel" />
            </oxy:PlotView.Model>
        </oxy:PlotView>
        <Button Content="Button" HorizontalAlignment="Left" Margin="421,271,0,0" VerticalAlignment="Top" Width="130" Height="59" Click="Button_Click"/>


    </Grid>

</Window>
